#define mod 1000000007
class Solution {
public:

    long long func(long long x , long long y)
    {
        if (y==0) return 1;
        long long ans = func(x,y/2);
        ans = ans*ans;
        ans = ans%mod;
        if (y%2) ans = ans*x;
        ans = ans%mod;
        return ans;
    }

    int countGoodNumbers(long long n) {
        
        long long even_count = (n+1)/2;
        long long odd_count = (n)/2;

        return (func(5,even_count)*func(4,odd_count))%mod;
    }
};
